{
  "name": "ngx-brazil",
  "version": "21.0.0",
  "type": "commonjs",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve dev",
    "build": "ng build",
    "build:app": "npm run copy && ng build packages",
    "dist": "npm run build:app && rimraf src/ngx-brazil",
    "build:lib": "rimraf dist && npm run build:pack",
    "build:pack": "ng-packagr -p ngx-brazil/ng-package.json",
    "build:sass": "npm rebuild sass",
    "publishnpm": "npm run build:lib && cd dist/ngx-brazil/ && npm publish",
    "test": "npm run build:lib && npm run dist && npm run e2e",
    "copy": "mkdir -p src/ngx-brazil/ && cp -R ngx-brazil/dist/* src/ngx-brazil",
    "testdemo": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --port 4205",
    "kill": "kill $(lsof -t -i:4205)"
  },
  "peerDependencies": {
    "rxjs": ">=6.5.3",
    "zone.js": ">=0.13.3"
  },
  "dependencies": {
    "@angular/common": "^21.0.6",
    "@angular/compiler": "^21.0.6",
    "@angular/core": "^21.0.6",
    "@angular/platform-browser": "^21.0.6",
    "@angular/platform-browser-dynamic": "^21.0.6",
    "rxjs-compat": ">=6.0.0",
    "tslib": ">=2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^21.0.4",
    "@angular-devkit/schematics": "^21.0.4",
    "@angular-eslint/builder": "^16.3.1",
    "@angular-eslint/eslint-plugin": "15.2.0",
    "@angular-eslint/eslint-plugin-template": "15.2.0",
    "@angular-eslint/schematics": "^18.0.1",
    "@angular-eslint/template-parser": "15.2.0",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.6",
    "@angular/forms": "^21.0.6",
    "@angular/language-service": "^21.0.6",
    "@angular/router": "^21.0.6",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "autoprefixer": "^10.2.3",
    "cpr": "^3.0.0",
    "eslint": "^8.31.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsdoc": "61.5.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine": "4.6.0",
    "jasmine-core": "4.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage": "^2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "ng-packagr": "^21.0.1",
    "postcss": "^8.2.4",
    "protractor": "~7.0.0",
    "rimraf": "^2.7.1",
    "sass": "^1.32.5",
    "tailwindcss": "^2.0.3",
    "ts-node": "~7.0.0",
    "tsickle": "0.46.3",
    "tslib": "^2.0.0",
    "typescript": "^5.9.3",
    "webpack": "^5.92.1",
    "webpack-cli": "^4.9.2",
    "zone.js": "^0.15.0"
  }
}
